programa {
// Algoritmo de troca de variáveis
//https://www.youtube.com/watch?v=V0CIZGCG7ho
inteiro x, y, aux
  funcao inicio() {
    x = 15
    y = 22
    // alogoritmo de  troca de valores de variáveis
     aux = y
     y = x
     x = aux
     escreva("x vale: " + x + "\n")
     escreva("y vale: " + y)
  }
}
